
<% if notice %>
    <p id="notice"><%= notice %></p>
<% end %>
<ul class="bxslider">
  <% @slides.each do |slide| %>
    <li><%= image_tag slide.url.url(:large).to_s %></li>
  <% end %>
</ul>

<div id='video-thumbs'>
    <div class='preview'>Video 1</div>
    <div class='preview'>Video 2</div>
    <div class='preview'>Video 3</div>
    <div class='preview'>Video 4</div>
</div>